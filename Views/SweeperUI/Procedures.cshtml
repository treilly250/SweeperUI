                                    <div class="scb-cell-medium-left" tooltip="{{procedureGroup.Description}}">{{procedureGroup.Description}}</div>
                                    <div class="scb-cell-right">{{procedureGroup.ProcedureGroupTotal}}</div>
                                    <div class="scb-cell-right">{{procedureGroup.ProcedureGroupMaximum}}</div>
                                    <div class="scb-cell-right">{{procedureGroup.ProcedureGroupAverage}}</div>
                                    <div class="scb-cell-center">{{procedureGroup.EventCount}}</div>
