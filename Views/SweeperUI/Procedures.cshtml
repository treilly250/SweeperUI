@{
    ViewBag.Title = "USHC Sweeper";
}

<div id="bodyContainer" ng-app="SweeperUIApp">
    <section id="content">
        <div class="container">
            <div class="row">
                <div class="text-center col-md-12" ng-controller="SweeperUICtrl">
                    <div class="scb-display-pager floatfix" ng-show="SCB.displayState == 'main'">
                        <div class="snapshot-btn" style="width:800px" ng-click="SCB.displayState = 'filters'">
                            <b>Filters</b>:&nbsp;
                            <span ng-show="SCB.startDate.length == 0 && SCB.endDate.length == 0 && SCB.lowerBoundICD.length == 0 && SCB.upperBoundICD.length == 0 && SCB.lowerBoundCPT.length == 0 && SCB.upperBoundCPT.length == 0 && SCB.lowerBoundClaimAmount.length == 0 && SCB.upperBoundClaimAmount.length == 0">No filtering in effect</span>
                            <span ng-show="SCB.startDate.length == 0 && SCB.endDate.length == 0 && !(SCB.lowerBoundICD.length == 0 && SCB.upperBoundICD.length == 0 && SCB.lowerBoundCPT.length == 0 && SCB.upperBoundCPT.length == 0 && SCB.lowerBoundClaimAmount.length == 0 && SCB.upperBoundClaimAmount.length == 0)">No claim date filtering in effect</span>
                            <span ng-show="SCB.startDate.length > 0 && SCB.endDate.length > 0">Claim dates from {{SCB.startDate}} through {{SCB.endDate}}</span>
                            <span ng-show="SCB.startDate.length > 0 && SCB.endDate.length == 0">Claim dates no earlier than {{SCB.startDate}}</span>
                            <span ng-show="SCB.startDate.length == 0 && SCB.endDate.length > 0">Claim dates no later than {{SCB.endDate}}</span>
                            <span ng-show="!(SCB.lowerBoundCPT.length == 0 && SCB.upperBoundCPT.length == 0)">&hellip;</span>
                            <span ng-show="SCB.lowerBoundCPT.length > 0 && SCB.upperBoundCPT.length > 0">CPT between {{SCB.lowerBoundCPT}} and {{SCB.upperBoundCPT}}</span>
                            <span ng-show="SCB.lowerBoundCPT.length > 0 && SCB.upperBoundCPT.length == 0">CPT at least {{SCB.lowerBoundCPT}}</span>
                            <span ng-show="SCB.lowerBoundCPT.length == 0 && SCB.upperBoundCPT.length > 0">CPT at most {{SCB.upperBoundCPT}}</span>
                            <span ng-show="!(SCB.lowerBoundClaimAmount.length == 0 && SCB.upperBoundClaimAmount.length == 0)">&hellip;</span>
                            <span ng-show="SCB.lowerBoundClaimAmount.length > 0 && SCB.upperBoundClaimAmount.length > 0">claim costs between ${{SCB.lowerBoundClaimAmount}} and ${{SCB.upperBoundClaimAmount}}</span>
                            <span ng-show="SCB.lowerBoundClaimAmount.length > 0 && SCB.upperBoundClaimAmount.length == 0">claim costs at least ${{SCB.lowerBoundClaimAmount}}</span>
                            <span ng-show="SCB.lowerBoundClaimAmount.length == 0 && SCB.upperBoundClaimAmount.length > 0">claim costs at most ${{SCB.upperBoundClaimAmount}}</span>
                        </div>
                    </div>
                    <div tabset>
                        <div class="floatfix" tab heading="Procedures By Code Group" select="tabSelectProceduresGroup();" ng-attr-active="SCB.ProceduresGroup.drillShow">
                            <div class="tab-contents">
                                <div class="floatfix scb-display headers" ng-show="SCB.displayState == 'main'">
                                    <div class="scb-cell-left" ng-click="clickProceduresGroupSort('ProcedureGroup')">Range<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ProcedureGroup' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ProcedureGroup' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-medium-left" ng-click="clickProceduresGroupSort('Description')">Description<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'Description' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'Description' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-right" ng-click="clickProceduresGroupSort('ProcedureGroupTotalRaw')">Claim Total<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ProcedureGroupTotalRaw' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ProcedureGroupTotalRaw' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-right" ng-click="clickProceduresGroupSort('ProcedureGroupMaximumRaw')">Claim Max<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ProcedureGroupMaximumRaw' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ProcedureGroupMaximumRaw' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-right" ng-click="clickProceduresGroupSort('ProcedureGroupAverageRaw')">Claim Avg<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ProcedureGroupAverageRaw' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ProcedureGroupAverageRaw' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-center" ng-click="clickProceduresGroupSort('EventCount')">Events<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'EventCount' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'EventCount' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-center" ng-click="clickProceduresGroupSort('ProcedureGroupCount')">Claims<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ProcedureGroupCount' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ProcedureGroupCount' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-center" ng-click="clickProceduresGroupSort('ClaimantCount')">Claimants<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ClaimantCount' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ClaimantCount' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <div class="scb-cell-center" ng-click="clickProceduresGroupSort('EmployeeCount')">Employees<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'EmployeeCount' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'EmployeeCount' && !SCB.ProceduresGroup.sortAsc }"></span></div>
                                    <!--<div class="scb-cell-center" ng-click="clickProceduresGroupSort('ProviderCount')">Providers<span ng-class="{'ushc-paper-icon-chevron-down':SCB.ProceduresGroup.sort == 'ProviderCount' && SCB.ProceduresGroup.sortAsc, 'ushc-paper-icon-chevron-up':SCB.ProceduresGroup.sort == 'ProviderCount' && !SCB.ProceduresGroup.sortAsc }"></span></div>-->
                                </div>
                                <div class="floatfix scb-display scb-grid" ng-show="SCB.displayState == 'main'" ng-repeat="procedureGroup in SCB.ProceduresGroupPage" ng-click="clickProceduresGroup(procedureGroup);">
