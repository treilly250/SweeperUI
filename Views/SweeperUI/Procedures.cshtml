                                    <div class="scb-cell-center">{{procedureGroup.ClaimantCount}}</div>
                                    <div class="scb-cell-center">{{procedureGroup.EmployeeCount}}</div>
                                    <!--<div class="scb-cell-center">{{procedureGroup.ProviderCount}}</div>-->
                                </div>
                                <div class="scb-display-pager floatfix" ng-show="SCB.displayState == 'main'">
                                    <div class="disabled">Page #</div>
                                    <div class="ushc-paper-icon-angle-double-left" ng-class="{disabled:SCB.ProceduresGroup.pageIndex == 0}" ng-click="clickProceduresGroupPage(0)"></div>
                                    <div ng-class="{active:SCB.ProceduresGroup.pageIndex == page - 1}" ng-repeat="page in SCB.ProceduresGroup.visiblePages" ng-click="clickProceduresGroupPage(page - 1)">{{page}}</div>
                                    <div class="ushc-paper-icon-angle-double-right" ng-class="{disabled:SCB.ProceduresGroup.pageIndex == SCB.ProceduresGroup.pages.length - 1}" ng-click="clickProceduresGroupPage(SCB.ProceduresGroup.pages.length - 1)"></div>
