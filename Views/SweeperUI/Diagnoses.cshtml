                        <div class="floatfix" tab heading="Diagnoses by ICD Code" select="tabSelectDiagnosesByICD();" ng-attr-active="SCB.DiagnosesICD.drillShow">
                            <div class="tab-contents">
                                <div class="floatfix scb-display headers" ng-show="SCB.displayState == 'main'">
                                    <div class="scb-cell-left" ng-click="clickDiagnosesICDSort('ICDCode')">Code<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'ICDCode' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'ICDCode' && !SCB.DiagnosesICD.sortAsc }"></span></div>
                                    <div class="scb-cell-medium-wide-left" ng-click="clickDiagnosesICDSort('DescriptionShort')">Description<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'DescriptionShort' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'DescriptionShort' && !SCB.DiagnosesICD.sortAsc }"></span></div>
                                    <div class="scb-cell-right" ng-click="clickDiagnosesICDSort('ClaimTotalRaw')">Claim Total<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'ClaimTotalRaw' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'ClaimTotalRaw' && !SCB.DiagnosesICD.sortAsc }"></span></div>
                                    <div class="scb-cell-right" ng-click="clickDiagnosesICDSort('ClaimMaximumRaw')">Claim Max<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'ClaimMaximumRaw' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'ClaimMaximumRaw' && !SCB.DiagnosesICD.sortAsc }"></span></div>
                                    <div class="scb-cell-right" ng-click="clickDiagnosesICDSort('ClaimAverageRaw')">Claim Avg<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'ClaimAverageRaw' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'ClaimAverageRaw' && !SCB.DiagnosesICD.sortAsc }"></span></div>
                                    <div class="scb-cell-center" ng-click="clickDiagnosesICDSort('EventCount')">Events<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'EventCount' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'EventCount' && !SCB.DiagnosesICD.sortAsc }"></span></div>
                                    <div class="scb-cell-center" ng-click="clickDiagnosesICDSort('ClaimCount')">Claims<span ng-class="{'ushc-paper-icon-chevron-down':SCB.DiagnosesICD.sort == 'ClaimCount' && SCB.DiagnosesICD.sortAsc, 'ushc-paper-icon-chevron-up':SCB.DiagnosesICD.sort == 'ClaimCount' && !SCB.DiagnosesICD.sortAsc }"></span></div>
