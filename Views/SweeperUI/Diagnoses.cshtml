                                    <div class="scb-cell-right">{{diagnosis.ClaimTotal}}</div>
                                    <div class="scb-cell-right">{{diagnosis.ClaimMaximum}}</div>
                                    <div class="scb-cell-right">{{diagnosis.ClaimAverage}}</div>
